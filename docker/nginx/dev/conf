
  location /geoserver1 {
        proxy_pass http://geoserver:8080/geoserver;
   }

   location /geoserver2 {
        proxy_pass http://geoserver:8080/geoserver/web;
        proxy_redirect off;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
   }

      location /geoserver3 {
        proxy_pass http://geoserver:8080/geoserver/web/; # doesnt reload geoserver css when you remove headers
         proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
   }
